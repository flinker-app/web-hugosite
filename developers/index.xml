 &lt;rect x="50" y="40" width="240" height="165" rx="5" fill="url(#screenGrad)"/>
 &lt;/g>

 &lt;!-- Monitor stand -->
 &lt;path d="M150 210 L190 210 L195 235 L145 235 Z" fill="#3d3d3d"/>
 &lt;ellipse cx="170" cy="238" rx="35" ry="6" fill="#2d2d2d"/>

 &lt;!-- 3D Building inside screen - Main Tower -->
 &lt;g transform="translate(95, 55)">
 &lt;!-- Back face -->
 &lt;path d="M70 25 L120 42 L120 145 L70 128 Z" fill="#0560cc" opacity="0.8"/>
 &lt;!-- Front face -->
 &lt;path d="M70 25 L70 128 L20 145 L20 42 Z" fill="url(#flinkerPrimary)"/>
 &lt;!-- Top face -->
 &lt;path d="M20 42 L70 25 L120 42 L70 59 Z" fill="#96BFFF"/>

 &lt;!-- Windows grid on front face -->
