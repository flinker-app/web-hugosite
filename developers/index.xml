 &lt;/g>
 &lt;/g>

 &lt;!-- Smaller building - left side -->
 &lt;g transform="translate(60, 105)">
 &lt;path d="M35 18 L60 27 L60 95 L35 86 Z" fill="#0560cc" opacity="0.7"/>
 &lt;path d="M35 18 L35 86 L10 95 L10 27 Z" fill="url(#flinkerPrimary)" opacity="0.85"/>
 &lt;path d="M10 27 L35 18 L60 27 L35 36 Z" fill="#96BFFF" opacity="0.85"/>
 &lt;rect x="16" y="40" width="8" height="10" rx="1" fill="url(#flinkerLight)" opacity="0.8"/>
 &lt;rect x="16" y="56" width="8" height="10" rx="1" fill="url(#flinkerLight)" opacity="0.8"/>
 &lt;rect x="16" y="72" width="8" height="10" rx="1" fill="url(#flinkerLight)" opacity="0.8"/>
 &lt;/g>

 &lt;!-- Wireframe/measurement overlays on screen -->
 &lt;g filter="url(#softGlow)">
