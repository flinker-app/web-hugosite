 &lt;rect x="28" y="55" width="12" height="16" rx="1" fill="url(#flinkerLight)"/>
 &lt;rect x="48" y="55" width="12" height="16" rx="1" fill="url(#flinkerLight)"/>
 &lt;rect x="28" y="78" width="12" height="16" rx="1" fill="url(#flinkerLight)"/>
 &lt;rect x="48" y="78" width="12" height="16" rx="1" fill="url(#flinkerLight)"/>
 &lt;rect x="28" y="101" width="12" height="16" rx="1" fill="url(#flinkerLight)"/>
 &lt;rect x="48" y="101" width="12" height="16" rx="1" fill="url(#flinkerLight)"/>
 &lt;rect x="28" y="124" width="12" height="14" rx="1" fill="url(#flinkerLight)"/>
