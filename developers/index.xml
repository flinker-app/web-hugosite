 &lt;stop offset="0%" style="stop-color:#0779FF;stop-opacity:1" />
 &lt;stop offset="100%" style="stop-color:#0560cc;stop-opacity:1" />
 &lt;/linearGradient>
 &lt;linearGradient id="flinkerLight" x1="0%" y1="0%" x2="100%" y2="100%">
 &lt;stop offset="0%" style="stop-color:#96BFFF;stop-opacity:0.7" />
 &lt;stop offset="100%" style="stop-color:#0779FF;stop-opacity:0.4" />
 &lt;/linearGradient>
 &lt;linearGradient id="flinkerGround" x1="0%" y1="0%" x2="0%" y2="100%">
 &lt;stop offset="0%" style="stop-color:#0779FF;stop-opacity:0.25" />
 &lt;stop offset="100%" style="stop-color:#96BFFF;stop-opacity:0.05" />
 &lt;/linearGradient>
 &lt;linearGradient id="screenGrad" x1="0%" y1="0%" x2="0%" y2="100%">
 &lt;stop offset="0%" style="stop-color:#1a1a2e;stop-opacity:1" />
 &lt;stop offset="100%" style="stop-color:#16213e;stop-opacity:1" />
 &lt;/linearGradient>
 &lt;filter id="softGlow" x="-50%" y="-50%" width="200%" height="200%">
 &lt;feGaussianBlur stdDeviation="2" result="coloredBlur"/>
 &lt;feMerge>
 &lt;feMergeNode in="coloredBlur"/>
 &lt;feMergeNode in="SourceGraphic"/>
 &lt;/feMerge>
 &lt;/filter>
 &lt;filter id="shadow" x="-20%" y="-20%" width="140%" height="140%">
 &lt;feDropShadow dx="0" dy="4" stdDeviation="8" flood-color="#0779FF" flood-opacity="0.2"/>
 &lt;/filter>
 &lt;/defs>

 &lt;!-- Ground reflection -->
 &lt;ellipse cx="170" cy="255" rx="130" ry="20" fill="url(#flinkerGround)" />

